@model usuario

<h1>Nuevo producto </h1>

<form action="" method="post">
    <div class="form-group">
        <label asp-for="Nombre"></label>
        <input asp-for="Nombre" type="text" class="form-control">
    </div>

   <div class="form-group">
        <label asp-for="Foto"></label>
        <input asp-for="Foto" class="form-control">
    </div>
   

 <div class="form-group">
        <label asp-for="categoria"></label>
        <select asp-for="arteId" asp-items="ViewBag.categoria" type="text" class="form-control"></select>
    </div>


    <button class="btn btn-primary">Registrar producto</button>
</form>